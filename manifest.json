{
  "manifest_version": 2,
  "name": "Racing World Builder App",
  "description": "Nick Ooms Racing World Builder App",
  "version": "3",
  "minimum_chrome_version": "23",
  "permissions": [
    {"fileSystem": ["write","retainEntries","directory"]},
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "48": "golf48x48.png",
    "128": "golf128x128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
